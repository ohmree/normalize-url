{
    "name": "@ohmree/normalize-url",
    "version": "7.0.4",
    "description": "Normalize a URL",
    "license": "MIT",
    "repository": "ohmree/normalize-url",
    "author": {
        "name": "ohmree",
        "url": "https://github.com/ohmree"
    },
    "type": "module",
    "module": "./index.js",
    "main": "./index.cjs",
    "engines": {
        "node": ">=12.20"
    },
    "scripts": {
        "build": "tsup index.js --format cjs --out-dir .",
        "test": "xo && c8 ava && tsd"
    },
    "types": "./index.d.ts",
    "files": [
        "index.js",
        "index.cjs",
        "index.d.ts"
    ],
    "keywords": [
        "normalize",
        "url",
        "uri",
        "address",
        "string",
        "normalization",
        "normalisation",
        "query",
        "querystring",
        "simplify",
        "strip",
        "trim",
        "canonical"
    ],
    "devDependencies": {
        "ava": "^4.0.1",
        "c8": "^7.11.0",
        "tsd": "^0.19.1",
        "tsup": "^5.12.8",
        "xo": "^0.49.0"
    },
    "c8": {
        "reporter": [
            "text",
            "lcov"
        ]
    },
    "xo": {
        "space": true,
        "prettier": true
    }
}
